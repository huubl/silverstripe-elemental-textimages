{
    "name": "derralf/elemental-textimages",
    "description": "A content block with HTML-Text and Image(s) for the silverstripe-elemental module",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "elemental",
        "content blocks"
    ],
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "dnadesign/silverstripe-elemental": "^4@dev",
        "silverstripe/vendor-plugin": "^1.0",
        "sheadawson/silverstripe-linkable": "^2.0@dev",
        "jonom/focuspoint": "^3.0",
        "bummzack/sortablefile": "^2.0"
    },
    "suggest": {
        "derralf/elemental-styling": "Adds some styling options"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "expose": [
            "client/dist",
            "icons"
        ]
    },
    "autoload": {
        "psr-4": {
            "Derralf\\Elements\\TextImages\\": "src/"
        }
    }

}
